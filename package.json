{
  "name": "mistral-format",
  "version": "1.0.5",
  "description": "A library for formatting responses from Mistral AI API",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophe77/mistral-format.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:tsc && npm run build:webpack && npm run copy:playground",
    "build:tsc": "tsc",
    "build:webpack": "webpack --config webpack.config.js --stats-error-details",
    "copy:playground": "node -e \"const fs = require('fs'); const path = require('path'); const dir = 'playground/dist'; if (!fs.existsSync(dir)) fs.mkdirSync(dir, { recursive: true }); fs.copyFileSync('dist/mistral-format.min.js', 'playground/dist/mistral-format.min.js');\"",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "dev": "tsc -w"
  },
  "keywords": [
    "mistral",
    "ai",
    "api",
    "formatter",
    "json",
    "xml",
    "markdown",
    "sql"
  ],
  "author": "christophe77",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.6.1",
    "nock": "^13.3.2",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "prettier": "^2.8.8",
    "process": "^0.11.10",
    "rimraf": "^5.0.1",
    "stream-browserify": "^3.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ]
}
